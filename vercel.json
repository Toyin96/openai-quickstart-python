{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@liudonghua123/now-flask",
      "config": {
        "output": {
          "file": "app.py",
          "command": "gunicorn app:app -w 4 -k uvicorn.workers.UvicornWorker"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}
